  "scripts": {
    "dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile ./src/",
    "start": "node express.js",
    "test:unit": "jest --coverage --silent",
    "test:unit:report": "jest --coverage --reporters=default --reporters=jest-junit --coverageReporters json-summary",
    "test:unit:watch": "jest --watch",
    "test:update-snapshot": "jest --updateSnapshot",
    "watch": "webpack --watch",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --mode development",
    "build:test": "webpack --config config/webpack.test.js --progress --profile --mode test",
    "build:prod": "webpack --config config/webpack.prod.js --progress --profile --mode production",
    "build:qa": "webpack --config config/webpack.qa.js --progress --profile --mode production",
    "build:staging": "webpack --config config/webpack.staging.js --progress --profile --mode production",
    "build:pr-staging": "webpack --config config/webpack.pr-staging.js --progress --profile --mode production",
    "build": "yarn run build:dev",
    "lint": "eslint -c ./.eslintrc.json  ./src --ext .ts,.tsx",
    "lint:fix": "eslint -c ./.eslintrc.json --fix  ./src --ext .ts,.tsx",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/__tests__/mock/browserMocks.js",
      "<rootDir>/src/__tests__/setup/test-shim.js",
      "<rootDir>/src/__tests__/setup/test-setup.js",
      "jest-canvas-mock"
    ],
    "globalSetup": "<rootDir>/src/__tests__/setup/global-setup.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jsdom",
    "timers": "legacy",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/setup/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__tests__/setup/styleMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/components/**/*.(ts|tsx|js)",
      "<rootDir>/src/services/**/*.(ts|tsx|js)"
    ],
    "coverageDirectory": "<rootDir>/src/__tests__/reports"
  },
  "jest-junit": {
    "outputDirectory": "<rootDir>/src/__tests__/reports",
    "outputName": "junit.xml"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
